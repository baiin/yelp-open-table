{
  "manifest_version": 2,
  "name": "Yelp Open Table",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.opentable.com/r/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js", "jquery-3.4.1.min.js"]
  }
}
